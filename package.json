{
    "name": "nest-template",
    "version": "0.1.0",
    "description": "NestJS API project template crafted for Docker environments",
    "author": "Saluki",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=14.0.0"
    },
    "scripts": {
        "clean": "rimraf ./dist",
        "start": "env-cmd -f .env nest start",
        "dev": "env-cmd -f .env nest start --watch",
        "debug": "env-cmd -f .env nest start --watch --debug",
        "build": "npm run clean && tsc",
        "test": "env-cmd -f .env.test jest --config=jest.json --coverage -i",
        "lint": "eslint .",
        "typeorm": "ts-node ./node_modules/typeorm/cli",
        "typeorm:run-migrations": "env-cmd -f .env  npm run typeorm migration:run -- -d ./src/modules/common/provider/config.provider.ts",
        "typeorm:generate-migration": "env-cmd -f .env  npm run typeorm migration:generate -- -d ./src/modules/common/provider/config.provider.ts ./src/migrations/$npm_config_name",
        "typeorm:create-migration": "npm run typeorm -- migration:create",
        "typeorm:revert-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert",
        "migration:create": "env-cmd -f .env ts-node ./node_modules/.bin/typeorm migration:create ./src/migrations-seeding/$npm_config_name",
        "migration:generate": "env-cmd -f .env ts-node ./node_modules/.bin/typeorm migration:generate -d ./src/modules/common/provider/config.provider.ts ./src/migrations/$npm_config_name",
        "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run -d ./src/modules/common/provider/config.provider.ts",
        "migration:revert": "env-cmd -f .env ts-node ./node_modules/.bin/typeorm migration:revert -d ./src/modules/common/provider/config.provider.ts",
        "migration:clean": "env-cmd -f .env ts-node ./scripts/clear-db.ts"
    },
    "dependencies": {
        "@nestjs/axios": "^3.0.0",
        "@nestjs/common": "^10.0.5",
        "@nestjs/config": "^3.0.0",
        "@nestjs/core": "^10.0.5",
        "@nestjs/jwt": "^10.1.0",
        "@nestjs/platform-express": "^10.0.5",
        "@nestjs/schedule": "^3.0.1",
        "@nestjs/swagger": "^7.1.1",
        "@nestjs/testing": "^10.0.5",
        "@nestjs/typeorm": "^10.0.0",
        "axios": "^1.5.0",
        "bcrypt": "^5.1.0",
        "better-sqlite3": "^8.5.0",
        "casual": "^1.6.2",
        "cookie-parser": "^1.4.6",
        "express": "^4.17.1",
        "helmet": "^4.6.0",
        "joi": "^17.5.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "mysql": "^2.18.1",
        "pg": "^8.11.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.4.0",
        "swagger-ui-express": "^4.2.0",
        "ts-jose": "^4.14.4",
        "typeorm": "^0.3.17",
        "typeorm-extension": "^2.8.1",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.0.2",
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cron": "^2.0.1",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.3",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/lodash": "^4.14.178",
        "@types/luxon": "^3.3.1",
        "@types/multer": "^1.4.7",
        "@types/node": "^16.0.0",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "env-cmd": "^10.1.0",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "husky": "^7.0.4",
        "jest": "^27.4.5",
        "nodemon": "^2.0.20",
        "prettier": "3.0.0",
        "rimraf": "^3.0.2",
        "supertest": "^6.1.6",
        "ts-jest": "^27.1.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    }
}
